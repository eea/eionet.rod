<?xml version="1.0" encoding="ISO-8859-1"?>

<rdf:RDF xmlns='http://rod.eionet.eu.int/schema#'
         xmlns:rdf='http://www.w3.org/1999/02/22-rdf-syntax-ns#'
         xmlns:eor='http://dublincore.org/2000/03/13/eor#'
         xmlns:rdfs='http://www.w3.org/2000/01/rdf-schema#'>

  <eor:Schema rdf:about=''>
    <rdf:value>Activities schema</rdf:value>
  </eor:Schema>

  <rdfs:Class rdf:ID='Obligation'>
    <rdfs:label>Reporting Obligation</rdfs:label>
  </rdfs:Class>

  <rdf:Property rdf:ID='obligation'>
    <rdf:value>Obligation name</rdf:value>
    <rdfs:label>Obligation name</rdfs:label>
    <rdfs:range rdf:resource='#Activity' />
  </rdf:Property>

  <rdfs:Class rdf:ID="Delivery" rdfs:label="Reportnet Delivery"/>
  <rdfs:Class rdf:ID="Referral" rdfs:label="Repository Referral"/>

    <rdfs:Class rdf:ID="Locality">
      <rdfs:label>Locality</rdfs:label>
      <rdfs:comment>This is a limited list of localities that deliver to the repository</rdfs:comment>
    </rdfs:Class>

    <rdf:Property rdf:ID="locality">
      <rdfs:label>Locality</rdfs:label>
      <rdfs:range rdf:resource="#Locality" />
      <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/coverage" />
    </rdf:Property>

</rdf:RDF>
