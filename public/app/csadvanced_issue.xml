<?xml version="1.0"?>
<xrs:XmlData xmlns:xrs="http://www.tietoenator.com/ee/2000/XRS">
	<xrs:RowSet name="Main">
		<T_ACTIVITY>
			<PK_RA_ID/>
			<TITLE/>
			<RESPONSIBLE_ROLE/>
			<DEADLINE expression="NEXT_DEADLINE AS DEADLINE"/>
			<DEADLINE2 expression="NEXT_DEADLINE2 AS DEADLINE2"/>
			<NEXT_REPORTING/>
			<FK_DELIVERY_COUNTRY_IDS/>
			<TERMINATE/>
		</T_ACTIVITY>
		<xrs:Constraint>TERMINATE='N'</xrs:Constraint>
		<T_ROLE alias="RESPONSIBLE" relation="CONCAT(T_ACTIVITY.RESPONSIBLE_ROLE,'-',LCASE(T_SPATIAL.SPATIAL_TWOLETTER))=RESPONSIBLE.ROLE_ID" reltype="outer">
			<ROLE_NAME/>
			<ROLE_EMAIL/>
			<ROLE_ID/>
			<ROLE_URL/>
			<ROLE_MEMBERS_URL/>
		</T_ROLE>
		<T_REPORTING relation="T_REPORTING.PK_RO_ID=T_ACTIVITY.FK_RO_ID">
			<PK_RO_ID/>
			<FK_SOURCE_ID/>
			<FK_CLIENT_ID/>
		</T_REPORTING>
		<T_CLIENT_LNK relation="T_CLIENT_LNK.TYPE='R' AND T_CLIENT_LNK.STATUS='M' AND T_CLIENT_LNK.FK_OBJECT_ID=T_REPORTING.PK_RO_ID" reltype="outer">
			<FK_CLIENT_ID/>
			<FK_OBJECT_ID/>
			<STATUS/>
			<TYPE/>
		</T_CLIENT_LNK>
		<T_CLIENT relation="T_CLIENT.PK_CLIENT_ID = T_CLIENT_LNK.FK_CLIENT_ID" reltype="outer">
			<PK_CLIENT_ID/>
			<CLIENT_NAME/>
		</T_CLIENT>
		<T_RASPATIAL_LNK relation="T_RASPATIAL_LNK.FK_RA_ID=T_ACTIVITY.PK_RA_ID">
			<FK_RA_ID/>
			<FK_SPATIAL_ID/>
		</T_RASPATIAL_LNK>
		<T_SPATIAL relation="T_RASPATIAL_LNK.FK_SPATIAL_ID=T_SPATIAL.PK_SPATIAL_ID">
			<PK_SPATIAL_ID/>
			<SPATIAL_NAME/>
			<SPATIAL_TWOLETTER/>
		</T_SPATIAL>
		<xrs:Constraint>PK_SPATIAL_ID like @COUNTRY_ID</xrs:Constraint>
		<T_RAISSUE_LNK relation="T_ACTIVITY.PK_RA_ID=T_RAISSUE_LNK.FK_RA_ID">
			<FK_RA_ID/>
			<FK_ISSUE_ID/>
		</T_RAISSUE_LNK>
		<xrs:Constraint>FK_ISSUE_ID=@ISSUE_ID</xrs:Constraint>
		<T_DELIVERY relation="T_ACTIVITY.PK_RA_ID=T_DELIVERY.FK_RA_ID AND T_DELIVERY.FK_SPATIAL_ID=T_SPATIAL.PK_SPATIAL_ID" reltype="outer">
			<PK_DELIVERY_ID/>
			<FK_SPATIAL_ID/>
			<FK_RA_ID/>
		</T_DELIVERY>
		<T_SOURCE relation="T_SOURCE.PK_SOURCE_ID = T_REPORTING.FK_SOURCE_ID" reltype="outer">
			<PK_SOURCE_ID/>
			<SOURCE_CODE/>
		</T_SOURCE>
		<xrs:Ordering>@ORD</xrs:Ordering>
	</xrs:RowSet>
	<xrs:RowSet name="EnvIssue">
		<T_ISSUE>
			<PK_ISSUE_ID/>
			<ISSUE_NAME/>
		</T_ISSUE>
		<xrs:Ordering>T_ISSUE.ISSUE_NAME</xrs:Ordering>
	</xrs:RowSet>
	<xrs:RowSet name="Dummy">
		<T_DUMMY>
			<DUMMY/>
		</T_DUMMY>
		<xrs:Constraint>DUMMY=@COUNTRY_ID</xrs:Constraint>
	</xrs:RowSet>
	<xrs:RowSet name="IssueData">
		<T_ISSUE>
			<ISSUE_NAME/>
		</T_ISSUE>
		<xrs:Constraint>PK_ISSUE_ID=@ISSUE_ID</xrs:Constraint>
	</xrs:RowSet>
	<xrs:RowSet name="CountryData">
		<T_SPATIAL>
			<SPATIAL_NAME/>
		</T_SPATIAL>
		<xrs:Constraint>PK_SPATIAL_ID=@COUNTRY_ID</xrs:Constraint>
	</xrs:RowSet>
</xrs:XmlData>
